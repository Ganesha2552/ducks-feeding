  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^6.0.1",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "jquery": "^3.6.0",
    "json-2-csv": "^3.10.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^5.12.3",
    "node-schedule": "^2.0.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react": "^17.0.2",
    "react-measure": "^2.5.2",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
